{
  "name": "TrackingSDA-Node",
  "version": "1.0.0",
  "author": "mariotsi",
  "description": "Tracking SDA shipment rest API",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./out/index.js",
    "build": "npm run copystatic && npm run tscompile",
    "tscompile": "tsc -p . ",
    "copystatic": "copyfiles -u 1 './src/**/*.hbs' './src/**/*.css' out",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "precommit": "tslint -c tslint.json 'src/**/*.ts'",
    "test:coverage": "istanbul cover ./node_modules/mocha/bin/_mocha ./test/**/*.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | codacy-coverage &&  codecov && rm -rf ./coverage",
    "test": "mocha ./test/**/*.js"
  },
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cheerio": "^0.22.2",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.36",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.2.34",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.0.12",
    "@types/request": "0.0.46",
    "@types/request-promise-native": "^1.0.6",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.2.0",
    "debug": "^2.6.9",
    "express": "^4.16.0",
    "hbs": "^4.0.1",
    "moment": "^2.19.3",
    "moment-timezone": "^0.5.13",
    "morgan": "^1.9.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.4",
    "serve-favicon": "^2.4.5",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "codacy-coverage": "^2.0.2",
    "codecov": "^2.2.0",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "mockery": "^2.1.0",
    "nodemon": "^1.11.0",
    "sinon": "^2.3.8",
    "tslint-config-airbnb": "^5.2.1"
  }
}
