{
  "name": "TrackingSDA-Node",
  "version": "1.0.0",
  "author": "mariotsi",
  "description": "Tracking SDA shipment rest API",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./out/index.js",
    "build": "npm run copystatic && npm run tscompile",
    "tscompile": "tsc -p . ",
    "copystatic": "copyfiles -u 1 './src/**/*.hbs' './src/**/*.css' out",
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "precommit": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/cheerio": "^0.22.2",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.36",
    "@types/morgan": "^1.7.32",
    "@types/node": "^6.0.81",
    "@types/request": "0.0.46",
    "@types/request-promise-native": "^1.0.6",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "^1.4.3",
    "copyfiles": "^1.2.0",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "hbs": "^4.0.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "serve-favicon": "^2.4.3",
    "typescript": "^2.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "tslint-config-airbnb": "^5.2.1"
  }
}
